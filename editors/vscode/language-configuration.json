{
  "comments": {
    "lineComment": "\"",
    "blockComment": null
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "'", "close": "'", "notIn": ["string", "comment"] },
    { "open": "`", "close": "`", "notIn": ["string", "comment"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["'", "'"],
    ["`", "`"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(IF|LOOP|DO|WHILE|TRY|CASE|CLASS|METHOD|FORM|FUNCTION|MODULE|INTERFACE|DEFINE|SELECT)\\b",
      "end": "^\\s*(ENDIF|ENDLOOP|ENDDO|ENDWHILE|ENDTRY|ENDCASE|ENDCLASS|ENDMETHOD|ENDFORM|ENDFUNCTION|ENDMODULE|ENDINTERFACE|END-OF-DEFINITION|ENDSELECT)\\b"
    }
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*|<[a-zA-Z_][a-zA-Z0-9_]*>"
}
