{
  "name": "tree-sitter-abap",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the ABAP programming language",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "abap",
    "sap"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.0"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "build": "tree-sitter generate && node-gyp rebuild"
  },
  "tree-sitter": [
    {
      "scope": "source.abap",
      "file-types": [
        "abap"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "tags": "queries/tags.scm"
    }
  ]
}
